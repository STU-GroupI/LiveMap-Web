@model LiveMapDashboard.Web.Models.Category.CategoryListViewModel

@{
    string? ParseValue(string name)
    {
        return ((TempData[name] ?? ViewData[name]) as string) switch
        {
            string value when value != string.Empty => value,
            _ => null
        };
    };
    
    ViewData["SuccessMessage"] = ParseValue("SuccessMessage");
    ViewData["ErrorMessage"] = ParseValue("ErrorMessage");
}

<vc:category-list
    model="@Model">
</vc:category-list>